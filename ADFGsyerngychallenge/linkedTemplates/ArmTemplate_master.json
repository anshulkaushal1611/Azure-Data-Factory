{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFGsyerngychallenge"
		},
		"Challengeinputlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Challengeinputlinkedservice'"
		},
		"challengesnowflakelinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'challengesnowflakelinkedservice'"
		},
		"challengesnowflakelinkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "GSYNERGY_CHALLENGE"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFGsyerngychallenge_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Challengeinputlinkedservice_connectionString": {
						"value": "[parameters('Challengeinputlinkedservice_connectionString')]"
					},
					"challengesnowflakelinkedservice_password": {
						"value": "[parameters('challengesnowflakelinkedservice_password')]"
					},
					"challengesnowflakelinkedservice_properties_typeProperties_database": {
						"value": "[parameters('challengesnowflakelinkedservice_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFGsyerngychallenge_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFGsyerngychallenge_ArmTemplate_0')]"
			]
		}
	]
}